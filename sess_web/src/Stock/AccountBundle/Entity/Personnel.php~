<?php
namespace Stock\AccountBundle\Entity;

class Personnel
{

    protected $id_number;
    protected $name;

    /**
     * Set id_number
     *
     * @param string $idNumber
     * @return Personnel
     */
    public function setIdNumber($idNumber)
    {
        $this->id_number = $idNumber;

        return $this;
    }

    /**
     * Get id_number
     *
     * @return string 
     */
    public function getIdNumber()
    {
        return $this->id_number;
    }

    /**
     * Set name
     *
     * @param string $name
     * @return Personnel
     */
    public function setName($name)
    {
        $this->name = $name;

        return $this;
    }

    /**
     * Get name
     *
     * @return string 
     */
    public function getName()
    {
        return $this->name;
    }
}
